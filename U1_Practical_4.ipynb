   "source": [
    "A Python Program to reverse a number (eg. n = 123456 -> 654321)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "2aa3417e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The Number: 12345678\n",
      "12345678 in reverse order is 87654321\n"
     ]
    }
   ],
   "source": [
    "num1= input(\"Enter a number: \")\n",
    "print(f\"The Number: {num1}\")\n",
    "length=len(num1)\n",
    "reverse=\"\"\n",
    "for a in range(length-1, -1, -1):\n",
    "    reverse+=num1[a]\n",
    "reverse = int(reverse)\n",
    "print(f\"{num1} in reverse order is {reverse}\")"
   ]
  }
