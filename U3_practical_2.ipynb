    "import math\n",
    "radius = int(input(\"Enter the radius of the circle: \"))\n",
    "print(f\"Radius: {radius}\")\n",
    "\n",
    "pi = math.pi\n",
    "\n",
    "print(f\"Circumference of the circle: {2*pi*radius}\")\n",
    "\n",
    "\n"
