   "source": [
    "A Python Program to count number of vowels in a string "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "15cbf39e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The word: Hello World, Python\n",
      "vowels: 4\n"
     ]
    }
   ],
   "source": [
    "word=input(\"Enter a word: \")\n",
    "print(f\"The word: {word}\")\n",
    "vowels = [\"a\", \"e\", \"i\",\"o\",\"u\"]\n",
    "count = 0\n",
    "for a in word:\n",
    "    if a in vowels:\n",
    "        count+=1\n",
    "print(f\"vowels: {count}\")"
   ]
  }
